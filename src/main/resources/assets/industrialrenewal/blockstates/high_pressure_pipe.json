{
  "multipart": [
    {
      "apply": {
        "model": "industrialrenewal:block/press_pipe/pressurized_tube_core"
      }
    },
    {
      "when": {
        "north": "true"
      },
      "apply": {
        "model": "industrialrenewal:block/press_pipe/con2_north",
        "uvlock": false
      }
    },
    {
      "when": {
        "east": "true"
      },
      "apply": {
        "model": "industrialrenewal:block/press_pipe/con2_north",
        "y": 90,
        "uvlock": false
      }
    },
    {
      "when": {
        "south": "true"
      },
      "apply": {
        "model": "industrialrenewal:block/press_pipe/con2_north",
        "y": 180,
        "uvlock": false
      }
    },
    {
      "when": {
        "west": "true"
      },
      "apply": {
        "model": "industrialrenewal:block/press_pipe/con2_north",
        "y": 270,
        "uvlock": false
      }
    },
    {
      "when": {
        "up": "true"
      },
      "apply": {
        "model": "industrialrenewal:block/press_pipe/con2_north",
        "x": 270,
        "uvlock": false
      }
    },
    {
      "when": {
        "down": "true"
      },
      "apply": {
        "model": "industrialrenewal:block/press_pipe/con2_north",
        "x": 90,
        "uvlock": false
      }
    }
  ]
}